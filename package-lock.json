{
  "name": "workspace-example",
  "version": "1.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "workspace-example",
      "version": "1.0.0",
      "workspaces": [
        "main",
        "sub"
      ]
    },
    "main": {
      "resolved": "git+ssh://git@github.com/packages/main.git",
      "dependencies": {
        "sub": "^1.0.0"
      }
    },
    "node_modules/main": {
      "resolved": "main",
      "link": true
    },
    "node_modules/sub": {
      "resolved": "sub",
      "link": true
    },
    "sub": {
      "version": "1.3.1",
      "resolved": "git+ssh://git@github.com/packages/sub.git"
    }
  },
  "dependencies": {
    "main": {
      "version": "file:main",
      "requires": {
        "sub": "^1.0.0"
      }
    },
    "sub": {
      "version": "file:sub"
    }
  }
}
